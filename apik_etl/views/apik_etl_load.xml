<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="apik_etl_extract_form" model="ir.ui.view">
        <field name="name">apik.etl.extract.form</field>
        <field name="model">apik.etl.extract</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="name"/>
                        <field name="source"/>
                        <field name="ttype"/>
                        <field name="ss_type"/>
                    </group>
                </group>

            </form>
        </field>
    </record>

    <record id="apik_etl_extract_tree" model="ir.ui.view">
        <field name="name">apik.etl.extract.tree</field>
        <field name="model">apik.etl.extract</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="source"/>
                <field name="ttype"/>
                <field name="ss_type"/>
            </tree>
        </field>
    </record>

</odoo>
