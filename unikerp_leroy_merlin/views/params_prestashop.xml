<?xml version="1.0" encoding="utf-8"?>
<odoo> 
    <record model="ir.ui.view" id="params_prestashop_form_view_ext">
        <field name="name">params.prestashop.form.view.ext</field>
        <field name="model">parametre.presta</field>
        <field name="inherit_id" ref="apik_prestashop.param_presta_form"/>
        <field name="arch" type="xml">
            <field name="rep_sauvegarde_fichier_erreur" position="after">
               <field name="repertoire_maj_of"/>
               <field name="rep_archive_maj_of"/>
            </field>
            <field name="nom_fichier_invoic_presta_export" position="after">
               <field name="nom_fichier_maj_of"/>
               <field name="nom_archive_maj_of"/>
            </field>
        </field>
    </record>
    <!--  -->
    <record model="ir.ui.view" id="params_edi_form_view_ext">
        <field name="name">params.edi.form.view.ext</field>
        <field name="model">parametre.edi</field>
        <field name="inherit_id" ref="apik_edi.param_edi_form"/>
        <field name="arch" type="xml">
            <field name="repertoire_envoi_edi" position="after">
               <field name="rep_prt"/>
            </field>
            <field name="nom_fichier_invoic_edi_export" position="after">
               <field name="nom_fichier_prt"/>
            </field>
        </field>
    </record>
</odoo>