<?xml version="1.0" ?>
<odoo noupdate="1">

    <record id="ir_cron_database_backup" model="ir.cron">
        <field name="name">Saas Client Database Backup</field>
        <field name="model_id" ref="model_saas_client_dashboard"/>
        <field name="code">model.cron_backup_database()</field>
        <field name="state">code</field>
        <field name="interval_type">days</field>
        <field name="nextcall" eval="datetime.strftime(datetime.today()+timedelta(days=1),'%Y-%m-%d 05:%M:%S')"/>
        <field name="user_id" ref="base.user_root"/>
        <field eval="-1" name="numbercall"/>
        <field eval="5" name="priority"/>
        <field eval="0" name="doall"/>
        <field eval="1" name="active"/>
        <field eval="1" name="interval_number"/>
    </record>

</odoo>
