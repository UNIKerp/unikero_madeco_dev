<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_account_config_settings_custom" model="ir.ui.view">
        <field name="name">view_account_config_settings_custom</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="l10n_fr_intrastat_product.view_account_config_settings" />
        <field name="arch" type="xml">
			<xpath expr="//div[@id='intrastat-settings']" position="inside">
                <div class="o_setting_left_pane" />
                <div class="o_setting_right_pane">
                    <div class="row">
                        <label for="surface_unit_id" class="col-md-5" />
                        <field name="surface_unit_id" /><!--options="{'no_open': True, 'no_create': True}" /-->
                    </div>
                </div>
            </xpath>
        </field>
    </record>	
</odoo>
