<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.module.category" id="backup_id">
        <field name="name">Backup Database</field>
        <field name="description">Access levels</field>
        <field name="sequence">3</field>
    </record>
    <record id="group_user" model="res.groups">
        <field name="name">User</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
        <field name="category_id" ref="auto_database_backup.backup_id"/>
    </record>
    <record id="group_manager" model="res.groups">
        <field name="name">Administrator</field>
        <field name="implied_ids" eval="[(4, ref('auto_database_backup.group_user'))]"/>
        <field name="category_id" ref="auto_database_backup.backup_id"/>
    </record>
</odoo>